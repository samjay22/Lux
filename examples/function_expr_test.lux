// Test function expressions

// Function expression in a table
local ops: table = {
    add = fn(a: int, b: int) -> int {
        return a + b
    },
    multiply = fn(a: int, b: int) -> int {
        return a * b
    }
}

print("Testing function expressions in tables")

// Call the functions
local sum := ops.add(5, 3)
print("5 + 3 = ")
print(sum)

local product := ops.multiply(5, 3)
print("5 * 3 = ")
print(product)

